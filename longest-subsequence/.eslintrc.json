{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": [
      "./tsconfig.json"
    ]
  },
  "rules": {
    "no-extra-semi": 0,
    "semi": 0,
    "@typescript-eslint/semi": [
      2,
      "always"
    ]
  }
}
